{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": [
                "PATCH",
                "POST",
                "GET",
                "OPTIONS",
                "DELETE"
            ]
        }
    ]
}


